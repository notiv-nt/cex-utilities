{
  "scripts": {
    "dev": "parcel watch src/index.ts src/*.html --public-url '.'",
    "build:parcel": "parcel build src/index.ts src/*.html --public-url '.'",
    "build": "npm run build:parcel && npm run zip",
    "clean": "rm -rf node_modules .parcel-cache",
    "format": "prettier --write src",
    "zip": "zip -r extension.zip ./dist ./static icon.png manifest.json"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.7.0",
    "@parcel/transformer-typescript-tsc": "^2.7.0",
    "@parcel/transformer-vue": "^2.7.0",
    "@types/chrome": "^0.0.197",
    "@types/lodash": "^4.14.185",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eventemitter2": "^6.4.9",
    "lodash": "^4.17.21",
    "parcel": "^2.7.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "typescript": "^4.8.3",
    "vue": "^3.2.39"
  },
  "dependencies": {
    "tailwindcss": "^3.1.8"
  }
}
